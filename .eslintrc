{
  "extends": "standard",
  "plugins": [
    "standard"
  ],
  "rules": {
    "no-console": 0,
    "no-undef": "error",
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "max-len": [
      "error",
      {
        "code": 140
      }
    ],
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "ignore",
        "named": "always"
      }
    ],
    "semi": [
      2,
      "never"
    ],
    "no-underscore-dangle": 0
  }
}
