  "homepage": "https://github.com/btomashvili/bxbet-backend/#readme",
  "scripts": {
    "migrate": "truffle compile && truffle migrate --reset",
    "lint": "eslint . --quiet",
    "build": "webpack",
    "dev": "webpack-dev-server",
    "start": "NODE_ENV=development DEBUG=* CONFIG=development nodemon ./bin/www",
    "start:testing": "DEBUG=* CONFIG=testing nodemon ./bin/www",
    "pm2_test": "NODE_ENV=testing DEBUG=* CONFIG=testing pm2 start ./bin/www --name bexbet_8001",
    "pm2_prod": "NODE_ENV=production CONFIG=production pm2 start ./bin/www --name bexbet_8001",
    "start:local": "CONFIG=development_local nodemon bin/www",
    "socket": "node socket/index.js",
    "socket_pm2": "pm2 start socket/index.js --name socket_server_8005",
    "swagger": "apidoc-swagger -i app -o public/swagger",
    "apidoc": "apidoc -i app -o public/doc",
    "test": "NODE_ENV=test mocha --timeout 10000",
    "seed:users": "DEBUG=* node cli/seeds/users.seed.js seed:users",
    "seed:games": "DEBUG=* node cli/seeds/games.seed.js seed:games",
    "seed:orders": "DEBUG=* node cli/seeds/orders.seed.js seed:orders",
    "finish:game": "DEBUG=* node cli/finishGame.js finish:game",
    "postinstall": "node -e \"var s=['../src'],d=['node_modules/app'],fs=require('fs');s.forEach(function(_,i){fs.existsSync(d[i])||fs.symlinkSync(s[i],d[i],'dir')});\""
  },
