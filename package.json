{
  "name": "bxbet",
  "version": "1.0.0",
  "description": "bxbet MVP",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btomashvili/bxbet-backend.git"
  },
  "keywords": [
    "Ethereum",
    "dapp",
    "voting"
